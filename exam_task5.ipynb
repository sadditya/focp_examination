    "unique_strings = set()\n",
    "\n",
    "while True:\n",
    "    text = input(\"Enter a string (press Enter to finish): \")\n",
    "    if text == \"\":\n",
    "        break\n",
    "    unique_strings.add(text)\n",
    "\n",
    "print(\"Number of unique strings entered:\", len(unique_strings))\n"
   ]
